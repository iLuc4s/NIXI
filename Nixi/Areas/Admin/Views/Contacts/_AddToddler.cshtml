@model Nixi.Areas.Admin.Models.AddToddlerViewModel

@if (Model != null)
{
        <table class="table">
            @foreach (var item in Model.Toddlers)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
                <td>
                <td>
                    <button class="btn btn-info btn-sm btn-danger" onclick="location.href='@Url.Action("AddAssociation", "Contacts", new { subject = Model.SubjectId, add = item.Id } )'">+</button>
                </td>
            </tr>
        }
        </table>
}

